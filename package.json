{
  "name": "ember-clash",
  "version": "0.0.2",
  "description": "",
  "author": "lazidog",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "build": "nest build",
    "start": "nest start",
    "dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "prod": "node dist/main",
    "lint": "biome lint src --no-errors-on-unmatched",
    "fmt": "biome format src --write --no-errors-on-unmatched",
    "check": "biome check --write --no-errors-on-unmatched",
    "typecheck": "tsc --noEmit",
    "postinstall": "yarn build && yarn prisma:gen",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "seed": "ts-node prisma/seed",
    "prisma:db:push": "prisma db push",
    "prisma:fmt": "prisma format",
    "prisma:gen": "prisma generate",
    "prisma:migrate": "prisma migrate dev --create-only",
    "prisma:migrate:data": "scaffdog generate"
  },
  "dependencies": {
    "@lazidog/utils": "^1.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@prisma/client": "^6.17.0",
    "mezon-sdk": "^2.8.20",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@quramy/prisma-fabbrica": "^2.2.1",
    "@types/node": "^24.3.0",
    "@types/uuid": "^9.0.5",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^8.2.2",
    "dotenv": "^16.5.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "prisma": "^6.17.0",
    "scaffdog": "^3.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}